{
  "name": "ilkka.github.io",
  "version": "2.0.0",
  "private": true,
  "description": "My github pages site",
  "main": "gulpfile.js",
  "browserify": {
    "transform": [
      "reactify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    // shim react so that 1) we can still require() it 2) it's a browser global so devtools work
    "react": "global:React"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ilkka/ilkka.github.com"
  },
  "author": "Ilkka Laukkanen <ilkka@fastmail.fm>",
  "license": "Apache License v2",
  "bugs": {
    "url": "https://github.com/ilkka/ilkka.github.com/issues"
  },
  "homepage": "https://github.com/ilkka/ilkka.github.com",
  "devDependencies": {
    "browserify": "^3.46.1",
    "browserify-shim": "^3.4.1",
    "cdnjs-cdn-data": "^0.1.0",
    "gulp": "^3.6.2",
    "gulp-browserify": "^0.5.0",
    "gulp-google-cdn": "^0.4.0",
    "gulp-livereload": "^1.5.0",
    "gulp-plumber": "^0.6.1",
    "gulp-rename": "^1.2.0",
    "gulp-uglify": "^0.3.0",
    "reactify": "^0.13.1",
    "uglify-js": "^2.4.13",
    "watchify": "^0.8.0"
  }
}
